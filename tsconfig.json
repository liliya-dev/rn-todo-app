{
  "compilerOptions": {
      "module": "commonjs",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "outFile": "../../built/local/tsc.js",
      "sourceMap": true,
      "esModuleInterop": true, 
      "target": "ESNext",
      "jsx": "react"
  },
  "include": [
      "src/**/*", "AppNavigation.tsx"
  ],
  "exclude": [
      "node_modules",
      "**/*.spec.ts"
  ]
}